      background: #f7f7f7 !important; } body { margin: 0; } strong { color: #000
      !important; } .italic { font-style: italic; font-weight: 500; } ol {
      padding-left: 36px; } .btn:visited { color: "white" !important; } .btn,
      .link:visited, .outline-btn { color: #fff !important; } p { color:
      #24292e; } table { width: 100%; } @media screen and (max-width: 760px) {
      .flexed table, .flexed td { text-align: -webkit-left; } .flexed { display:
      grid; row-gap: 0.5rem; } .flexed .f-logo { margin-left: 0.4rem; } .flexed
